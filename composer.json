{
    "name": "roqmeu/symfony-roadrunner-workshop-2025-04",
    "type": "project",
    "description": "Podlodka PHP Crew: Optimizing Symfony with RoadRunner",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "require": {
        "php": "^8.3",
        "ext-amqp": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-yaml": "*",
        "ext-zip": "*",
        "baldinof/roadrunner-bundle": "^3.2",
        "doctrine/dbal": "^3.9",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/orm": "^3.0",
        "ramsey/uuid": "^4.0",
        "roqmeu/roadrunner-messenger": "^1.0",
        "spiral/roadrunner-cli": "^2.6",
        "symfony/amqp-messenger": "^7",
        "symfony/cache": "^7",
        "symfony/console": "^7",
        "symfony/dotenv": "^7",
        "symfony/expression-language": "^7",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7",
        "symfony/http-client": "^7",
        "symfony/intl": "^7",
        "symfony/lock": "^7",
        "symfony/messenger": "^7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "^7",
        "symfony/scheduler": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/string": "^7",
        "symfony/translation": "^7",
        "symfony/validator": "^7",
        "symfony/yaml": "^7"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1.60"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "*/*": false
        },
        "optimize-autoloader": false,
        "platform": {
            "php": "8.3.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "docker": false,
            "require": "7.2.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/roqmeu/roadrunner-messenger.git"
        }
    ]
}
